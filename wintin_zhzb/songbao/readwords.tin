#class readwords open;
#al timeouttu {timeout};

#unvar no_wabao_rooms;
#list no_wabao_rooms[豹头山] add {密洞};
#list no_wabao_rooms[傲来国] add {水帘洞内};
#list no_wabao_rooms[将军府] add {模糊中;茅屋};
#list no_wabao_rooms[蟠桃园] add {蟠桃树上};
#list no_wabao_rooms[无底洞] add {石洞;洞穴;狱门;石洞深处;狱中};
#list no_wabao_rooms[女儿国] add {迷宫出口};
#list no_wabao_rooms[五庄观] add {亭底秘室};
#list no_wabao_rooms[大雪山] add {地下暗室};

#var path_songbao {fly changan;{u;ask jin buhuan about bao}};
#al {do_s} {@item{path_now;$path_step};#math path_step $path_step+1};
#al {gotosongbao}
{exert recover;exert refresh;
tick 10;#var path_step 1;#list {path_now} {creat} {$path_songbao};#var path_steps @numitems{path_now};wait songbao_s 0.5};

#al {songbao_s} {#if {$path_step<=$path_steps} {wait do_s 0.005}};
#ac {^%1一把抓住了你！} {#if {"%1"!="%*【%*" && "%1"!="%*：%*" && "%1"!="%*>%*"} {#var blockguai_name %1;T+ blockguai.tin;id here;#math path_step $path_step-1}};
#if $v_xiudao==2 {#var path_songbao {fly changan;e;e;e;e;e;zuan;step;n;ne;ne;e;{xiudao 270;dive};wait u 2;fly changan;{u;ask jin buhuan about bao}}};
#ac {^%s这里%*的出口是} {tick 60;#if {$path_step<=$path_steps} {wait do_s 0.01}};
#ac {^%s这里没有任何明显的出路。} {#if {$path_step<=$path_steps} {wait do_s 0.005}};
#ac {^%?%s这个方向没有出路} {#al do_next {wait do_s 0.001};#if {$path_step<=$path_steps} {do_next}};
#ac {^你正忙着呢，没工夫腾云驾雾。} {#math path_step {$path_step-1};wait do_s 1};
#ac {^你的动作还没有完成，不能移动。} {#math path_step {$path_step-1};wait do_s 1};
#ac {^你要对谁耳语些什么？} {tick 5};
#ac {^你的法力不足！} {#if {$v_extra_catch>0} {feel $fabao_id2 for extra_catch;charge $fabao_id2 for extra_catch $v_extra_catch_charge} {get ling zhi from hualan;eat ling zhi};#math path_step {$path_step-1};wait do_s 1};

#ac {^金不换对你说：现在没人需要宝物，请稍等一会！} {#if @time_s{}>280 {tick 10;wait {ask jin buhuan about bao} 1} {d;gotolijing}};

#ac {^金不换对你说：上个任务还没完成也别着急要下一个任务啊。} {#if @time_s{}>580 {tick 10;wait {ask jin buhuan about bao} 5} {d;gotolijing}};

#ac {^金不换对你说：先把你身上的宝盒送出去再说吧。} {tick 5;d;d;drop treasure box;u;u;ask jin buhuan about bao};


#var treasure_map 0;
#ac {^金不换对你说道：利用此铲挖到宝盒并送给%1(%2),任务就完成了。} 
{tick 10;delwait;#math n_songbao $n_songbao+1;#var songbao_find 0;#var wabao_find 0;#var songbao_sta 准备挖宝;#var time_songbaostart @time{};
#var songbao_name %1;#var songbao_id %2;#var songbao_giveid %2;#var songbao_searchname {%1(@upper_f{%2})};
#replace songbao_id { } {-};#var songbao_nameid {%1(@upper_f{$songbao_id})};
#var songbao_do $list_songbao_do[@ismember{$songbao_nameid;list_songbao_name}];
songbao
};

#ac {^当前送宝：%1} {tick 10;#if {"%1"=="$songbao_name"} {read words} {#var songbao_name %1;#var songbao_id $list_songbao_id[@ismember{$songbao_name;list_songbao_back}];#var songbao_id1 $songbao_id;#replace songbao_id1 {-} { };#var songbao_giveid $songbao_id1;#var songbao_searchname {%1(@upper_f{$songbao_giveid)}};#var songbao_do $list_songbao_do[@ismember{$songbao_name;list_songbao_back}];#sh $songbao_name $songbao_id $songbao_giveid $songbao_searchname $songbao_do;songbao}};

#ac {^你看不清楚上面的字。} {tick 10;set public 宝盒地点：$wabao_room-$wabao_where。};
#high {^只见上面歪歪扭扭的写着：某年某月某日，藏宝于%+} {light green};
